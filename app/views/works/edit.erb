<h2>Edit New Work</h2>
<form action="/@artist.name %>/new" method="POST">
  <input type="hidden" name="_method" value="PATCH">
  <p>Work Title: <input type="text" name="title" value="<%= @work.title %>"></p>
  <p>Year: <input type="text" name="year" value="<%= @work.year %>"></p>
  <p>Medium: <input type="text" name="medium" value="<%= @work.medium %>"></p>
  <p>Price: <input type="text" name="price" value="<%= @work.price %>"></p>
  <input type="submit" name="submit" value="Save Work">
</form>
